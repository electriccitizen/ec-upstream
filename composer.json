{
    "name": "electriccitizen\/d9-upstream",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https:\/\/packages.drupal.org\/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "drush-ops/behat-drush-endpoint": "^9.4",
        "drush/drush": "^10",
        "pantheon-upstreams/upstream-configuration":"*" ,
        "drupal/addanother": "^1.0",
        "drupal/address": "^1.9",
        "drupal/addtoany": "^1.4",
        "drupal/admin_toolbar": "^3.0",
        "drupal/allowed_formats": "^1.3",
        "drupal/antibot": "^1.4",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/cacheflush": "^1.0@beta",
        "drupal/components": "^2.0",
        "drupal/config_filter": "^2.0",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "^2.0",
        "drupal/config_update": "^1.5",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9.0.0",
        "drupal/core-recommended": "^9",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.4",
        "drupal/default_content": "^2.0",
        "drupal/devel": "^4.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity": "^1.1",
        "drupal/entity_embed": "^1.1",
        "drupal/entity_reference_revisions": "^1.8",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/extlink": "^1.5",
        "drupal/field_group": "^3.1",
        "drupal/flippy": "^1.0",
        "drupal/focal_point": "^1.5",
        "drupal/geolocation": "^3.2.0",
        "drupal/hreflang": "^1.3",
        "drupal/image_style_quality": "^1.4",
        "drupal/layout_builder_restrictions": "^2.7",
        "drupal/linkit": "^6.0@beta",
        "drupal/menu_block": "^1.6",
        "drupal/metatag": "^1.14",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/pathologic": "^1.0",
        "drupal/quicklink": "^1.3",
        "drupal/redirect": "^1.6",
        "drupal/responsive_preview": "^1.0",
        "drupal/role_delegation": "^1.1",
        "drupal/search_api": "^1.18",
        "drupal/smart_date": "^3.1",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/superfish": "^1.4",
        "drupal/token": "^1.7",
        "drupal/twig_tweak": "^2.8",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/webform": "^6.0",
        "drupal/xmlsitemap": "^1.0"
    },
    "conflict": {
        "drupal\/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web\/core": [
                "type:drupal-core"
            ],
            "web\/libraries\/{$name}": [
                "type:drupal-library"
            ],
            "web\/modules\/composer\/{$name}": [
                "type:drupal-module"
            ],
            "web\/profiles\/composer\/{$name}": [
                "type:drupal-profile"
            ],
            "web\/themes\/composer\/{$name}": [
                "type:drupal-theme"
            ],
            "drush\/Commands\/contrib\/{$name}": [
                "type:drupal-drush"
            ],
            "web\/private\/scripts\/quicksilver\/{$name}\/": [
                "type:quicksilver-script"
            ]
        },
        "patches": {
            "drupal/core": {
                "db version": "https://raw.githubusercontent.com/stevector/drupal-9-project/master/patches/issue-2--mariadb-version.patch",
                "Update module can get 'stuck' with 'no releases available', https://www.drupal.org/project/drupal/issues/2920285": "https://www.drupal.org/files/issues/2920285-update-status.patch"
            },
            "drupal/entity_embed": {
                "Array to string conversion for Media Image": "https://www.drupal.org/files/issues/2019-10-18/entity-embed-array-to-string--3069448-19.patch"
            },
            "drupal/focal_point": {
                "Preview modal closes media library": "https://www.drupal.org/files/issues/2020-10-06/3162210-17.patch"
            },
            "drupal/responsive_preview": {
                "3229548 - 404 when trying to serve Drupal as subfolder": "https://www.drupal.org/files/issues/2021-08-24/404_drupal_from_subfolder_3229548-2.patch"
            },
            "drupal/token": {
                "Responsive Preview Integration": "https://www.drupal.org/files/issues/2021-08-13/responsive_preview_support_3218969-6.patch"
            }
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal\/core": "-p2"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems\/drupal-integrations"
            ],
            "locations": {
                "web-root": ".\/web"
            },
            "file-mapping": {
                "[project-root]\/.editorconfig": false,
                "[project-root]\/pantheon.upstream.yml": false,
                "[project-root]\/.gitattributes": false
            }
        },
        "build-env": {
            "export-configuration": "drush config-export --yes",
            "install-cms": [
                "drush site-install citizen --account-mail=no-reply@electriccitien.com --account-name=admin  --site-mail=no-reply@electriccitizen.com --site-name=d9-upstream --yes"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "scripts": {
        "unit-test": "echo 'No unit test step defined.'",
        "lint": "find web\/modules\/custom web\/themes\/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": [
            ".\/vendor\/bin\/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .\/web\/modules\/custom",
            ".\/vendor\/bin\/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .\/web\/themes\/custom",
            ".\/vendor\/bin\/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .\/web\/modules\/custom",
            ".\/vendor\/bin\/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .\/web\/themes\/custom"
        ]
    },
    "require-dev": {
        "behat/behat": "^3.9",
        "behat/mink": "^1.9",
        "behat/mink-extension": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "dmore/behat-chrome-extension": "^1.3",
        "drupal/coder": "^8.3",
        "drupal/drupal-driver": "^2.1",
        "drupal/drupal-extension": "^4.1",
        "genesis/behat-fail-aid": "^3.7",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.3",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/css-selector": "^5.3"
    }
}

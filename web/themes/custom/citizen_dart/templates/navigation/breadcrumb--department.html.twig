{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}

{% extends 'breadcrumb.html.twig' %}

{% block trail %}
  <li>
    <a href="/">Home</a>
  </li>
  <li>
    <a href="{{ path('entity.node.canonical',{'node':66}) }}">Departments</a>
  </li>
  <li>{{ currentNode.title.value }}</li>
{% endblock %}
